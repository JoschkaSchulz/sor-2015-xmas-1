<p id="notice"><%= notice %></p>

<h1>Listing Wunsches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wunsches.each do |wunsch| %>
      <tr>
        <td><%= link_to 'Show', wunsch %></td>
        <td><%= link_to 'Edit', edit_wunsch_path(wunsch) %></td>
        <td><%= link_to 'Destroy', wunsch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wunsch', new_wunsch_path %>
